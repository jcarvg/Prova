<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<!--Grouping Example-->
<suite name="suite">
    <test name="Suite de Testes">
        <groups>
            <run>
                <include name="healthCheck"/>
                <include name="contrato"/>
                <include name="funcional"/>
            </run>
        </groups>
    </test>
</suite>